{
    "opts": {
        "destination": "doc/",
        "recurse": true
    },
    "source": {
        "include": ["src/Component" , "src/Pages"],
        "includePattern": "\\.(jsx|js|ts|tsx)$",
        "excludePattern": "(node_modules/|doc)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks" : true
      }
}